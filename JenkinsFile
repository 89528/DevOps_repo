node{
  stage('Fetch code') {
        checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/itaifrenkel/java-hello-world-webapp/']]]) 
  }
  stage('Build') {
       sh 'mvn clean package'
  }
  stage('Test'){
      sh 'mvn test'
  }
  stage('Deploy to docker') {
      docker build -t ApacheProject -f /home/ashish/Downloads/docker/apache1 /home/ashish/Downloads/docker/
      docker run -d ApacheProject -p 80:80 -t webproj 
  }
}
